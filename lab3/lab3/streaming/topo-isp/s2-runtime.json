{
  "target": "bmv2",
  "p4info": "build/streaming.p4.p4info.txt",
  "bmv2_json": "build/streaming.json",
  "table_entries": [
  {
      "table": "MyIngress.ipv4_lpm",
      "default_action" : true,
      "action_name" : "MyIngress.drop",
      "action_params" : {}
    },
    {
      "table" : "MyIngress.ipv4_lpm",
      "match": {
        "hdr.ipv4.dstAddr": [
		"10.0.7.0",
		24
	]
      },
      "action_name": "MyIngress.multi",
      "action_params": {
      }
    },

    {
      "table" : "MyIngress.ipv4_lpm",
      "match": {
        "hdr.ipv4.dstAddr": [
		"10.0.1.0",
		24
	]
      },
      "action_name": "MyIngress.ipv4_forward",
      "action_params": {
      	"dstAddr" : "08:00:00:00:04:44",
	"port" : 1
      }
    },
    {
	"table" : "MyEgress.nat_table",
	"default_action" : true,
	"action_name" : "MyEgress.NoAction",
	"action_params" : {}
    },
    {
	    "table" : "MyEgress.nat_table",
	    "match" : {
		    "standard_metadata.egress_rid" : 1,
		    "standard_metadata.egress_port" : 3
	    },
	    "action_name" : "MyEgress.apply_nat",
	    "action_params" : {
		"dst" : "10.0.3.3"
		}
    }

  ],
  "multicast_group_entries" : [
   {
	"multicast_group_id" : 1,
	"replicas" : [
		{
		    "egress_port" : 2,
		    "instance" : 1
		},
		{
		    "egress_port" : 3,
		    "instance" : 1
		}
	]
   }
  ]
}
